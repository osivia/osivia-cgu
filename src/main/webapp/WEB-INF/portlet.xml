<?xml version="1.0" encoding="UTF-8"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
version="1.0">

	<portlet>
		<description>Portlet Cartoun Création Compte</description>
		<portlet-name>toutatice-cartoun-creationcompte-portailPortlet</portlet-name>
        <display-name>Cartoun - Création Compte</display-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>

		<!--  Fichier de configuration Spring de l'application --> 
		<init-param>
			<name>contextConfigLocation</name>
			<value>
			classpath:annuaire-beans.xml
			/WEB-INF/applicationContext.xml</value>
		</init-param>
		
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>View</portlet-mode>
			<portlet-mode>Admin</portlet-mode>
		</supports>
		
		<portlet-info>
			<title>Cartoun - Création Compte</title>
		</portlet-info>
	</portlet>
	
	
	
	
	<portlet>
		<portlet-name>toutatice-cartoun-maj-comptePortlet</portlet-name>
        <portlet-class>fr.toutatice.cartoun.portail.majcompte.SupportFeederPortlet</portlet-class>

		<!--  Fichier de configuration Spring de l'application --> 
		<init-param>
			<name>contextConfigLocation</name>
			<value>
			classpath:annuaire-beans.xml
			/WEB-INF/applicationContext.xml</value>
		</init-param>
		
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>View</portlet-mode>
			<portlet-mode>Admin</portlet-mode>
		</supports>
		
		<portlet-info>
			<title>Cartoun - Création Compte</title>
		</portlet-info>
	</portlet>
	

	
	
</portlet-app>
